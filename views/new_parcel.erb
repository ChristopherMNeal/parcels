<h1>Create a new Parcel</h1>

<form action="/parcels" method="post">
  <div class="form-group">
    <label for="parcel_name">Parcel name</label>
    <input id="n" name="parcel_name" class="form-control" type="text">
    <label for="parcel_weight">Parcel weight</label>
    <input id="p_w" name="parcel_weight" class="form-control" type="number">
    <label for="parcel_width">Parcel width</label>
    <input id="w" name="parcel_width" class="form-control" type="number">
    <label for="parcel_length">Parcel length</label>
    <input id="l" name="parcel_length" class="form-control" type="number">
    <label for="parcel_height">Parcel height</label>
    <input id="h" name="parcel_height" class="form-control" type="number">
  </div>
  <button type="submit" class="btn">Go!</button>
</form>